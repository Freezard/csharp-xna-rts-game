using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace RTSgame.Utilities.Memory
{
    public abstract class AlwaysAllocatedObject
    {
        private UInt16 id;

        public AlwaysAllocatedObject()
        {
        }

        internal void SetMemoryId(UInt16 Id)
        {
            id = Id;
        }

        public UInt16 GetMemoryId()
        {
            return id;
        }
    }
}
